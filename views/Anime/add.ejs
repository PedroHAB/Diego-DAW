<%-include('../cabecalho.ejs')%>
    <form method="post" enctype="multipart/form-data">
        Título: <input type="text" name="nome" 
            placeholder="Digite seu Nome" required ><br>
        Descrição: <input type="text" name="Descricao" 
            placeholder="Digite a descrição" required ><br>   
        DataL: <input type="date" name="DataL" 
            placeholder="Bote a data de lançamento" required ><br>    
        DataF: <input type="date" name="DataF" 
            placeholder="Bote a data de lançamento" required ><br>      
        NumEp: <input type="number" name="NumeroEp" 
            placeholder="Digite o número de Ep" required><br>
        Foto: <input type="file" name="banner" 
            accept="image/*" required><br>
        <input type="submit" value="Salvar">
    </form>
<%-include('../rodape.ejs')%>